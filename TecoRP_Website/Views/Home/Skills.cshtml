@model IEnumerable<TecoRP.Models.Jobs>
    @{
        ViewBag.Title = "Skills";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }




    @foreach (var item in Model)
    {
        <div class="demo-cards mdl-cell mdl-cell--4-col mdl-cell--8-col-tablet mdl-grid mdl-grid--no-spacing">
            <div class="demo-updates mdl-card mdl-shadow--2dp mdl-cell mdl-cell--4-col mdl-cell--4-col-tablet mdl-cell--12-col-desktop">
                <div style="background-image:url(/Content/images/karakterim.png);" class="mdl-card__title mdl-card--expand mdl-color--teal-300">
                    <h2 class="mdl-card__title-text">@item.GetJobName()</h2>
                </div>
                <div class="mdl-card__supporting-text mdl-color-text--grey-600">
                    Seviye: @item.JobLevel &nbsp;| Tamamlanan: @item.JobsCompleted  <br>
                </div>
            </div>
            <div class="demo-separator mdl-cell--1-col"></div>
        </div>
    }